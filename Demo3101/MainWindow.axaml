					  EndPoint="0,707"
					  Stroke="Black"
					  StrokeThickness="2"
					  Grid.RowSpan="13"/>
				<!--ЗАГОЛОВОК: Дороги России-->
				<Button Grid.Row="0"
						HorizontalAlignment="Left"
						Height="50"
						Width="310"
						Margin="0 3 0 0"
						CornerRadius="0"
						Background="Green"
						Name="mainButton"
						Tag="0">

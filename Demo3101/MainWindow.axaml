					<Line StartPoint="50,0"
						  EndPoint="0,0"
						  Stroke="Black"
						  StrokeThickness="2"
						  VerticalAlignment="Center"/>
					<Button Grid.Row="0"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Height="50"
							Width="310"
							CornerRadius="0"
							Margin="0 3 0 0"
							Background="#e1f4c8"

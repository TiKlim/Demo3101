<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="1500" d:DesignHeight="850"
		MinHeight="850"
		MinWidth="1500"
		MaxHeight="850"
		MaxWidth="1500"
		WindowStartupLocation="CenterScreen"
        x:Class="Demo3101.MainWindow"
		x:CompileBindings="False"
        Title="Demo3101">
	<!--Главный Grid-->
	<Grid ColumnDefinitions="*,*"
		  RowDefinitions="Auto,*"
		  ShowGridLines="False">
		<!--Шапка окна-->
		<DockPanel Background="#e1f4c8"
				   Grid.ColumnSpan="2"
				   Grid.Row="0">
			<Border Background="#78b24b"
					Height="70"
					Width="70"
					CornerRadius="90"
					Margin="5">
				<TextBlock Text="логотип"
						   HorizontalAlignment="Center"
						   VerticalAlignment="Center"/>
			</Border>
			<Border Background="White"
					Height="35"
					Width="608"
					CornerRadius="12">
				<TextBlock Text="Организационная структура"
						   HorizontalAlignment="Left"
						   VerticalAlignment="Center"
						   Margin="15 0 0 0"/>
			</Border>
		</DockPanel>
		<!--Правая сторона - сотрудники-->
		<Border Grid.Column="1"
				Grid.Row="1"
				Background="LightGray"
				Margin="15">
			<Grid RowDefinitions="*,Auto">
				<ListBox Grid.Row="0"
						 Background="LightGray"
						 Margin="5"
						 Name="staffLB">
					<ListBox.ItemTemplate>
						<DataTemplate>
							<Border Background="#e1f4c8">
								<Grid RowDefinitions="*,*,*,*">
									<DockPanel Grid.Row="0"
											   Margin="15 15 0 0">
										<TextBlock Text="{Binding GetNameDivision}"></TextBlock>		
										<TextBlock Text=" - "/>
										<TextBlock Text="{Binding GetNameRole}"
												   TextWrapping="Wrap"/>
									</DockPanel>
									<TextBlock Text="{Binding Iddepats.DivisionName}"
											   Grid.Row="0"
											   Margin="15 15 0 0"/>
									<DockPanel Grid.Row="1"
											   Margin="15 0 0 0">
										<TextBlock Text="{Binding StaffSurname}"
												   FontSize="18"/>
										<TextBlock Text=" "
												   FontSize="18"/>
										<TextBlock Text="{Binding StaffName}"
												   FontSize="18"/>
										<TextBlock Text=" "
												   FontSize="18"/>
										<TextBlock Text="{Binding StaffPatronimic}"
												   FontSize="18"/>
									</DockPanel>
									<DockPanel Grid.Row="2"
											   Margin="15 0 0 0">
										<TextBlock Text="{Binding StaffWorkPhone}"/>
										<TextBlock Text="   "/>
										<TextBlock Text="{Binding CorporateEmail}"/>
									</DockPanel>
									<TextBlock Text="{Binding StaffOfficeNavigation.OfficeName}"
											   Grid.Row="3"
											   Margin="15 0 0 15"/>
								</Grid>
							</Border>
						</DataTemplate>
					</ListBox.ItemTemplate>
				</ListBox>
				<!--Кнопка добавления нового сотрудника-->
				<Button Grid.Row="1"
						Background="#e1f4c8"
						CornerRadius="0"
						Height="100"
						Width="100"
						HorizontalAlignment="Right"
						Margin="5"
						Name="add">
					<TextBlock Text="+"
							   HorizontalAlignment="Center"
							   VerticalAlignment="Center"
							   FontSize="50"/>
				</Button>
			</Grid>
		</Border>
		<!--Левая сторона - диаграмма с отделами-->
		<Border Grid.Column="0"
				Grid.Row="1"
				Background="LightGray"
				Margin="15"
				IsVisible="True">
			<Grid RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*"
				  ShowGridLines="False"
				  Margin="5">

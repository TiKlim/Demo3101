						   HorizontalAlignment="Left"
						   Margin="0 20 0 0">
					<Line StartPoint="50,0"
						  EndPoint="0,0"
						  Stroke="Black"
						  StrokeThickness="2"
						  VerticalAlignment="Center"/>
					<Button Grid.Row="0"

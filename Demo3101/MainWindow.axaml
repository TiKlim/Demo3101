				Grid.Row="1"
				Background="LightGray"
				Margin="15"
				Name="firstDep"
				IsVisible="False">
			<Grid RowDefinitions="*,*,*,*,*,*,*,*,*,*"
				  ShowGridLines="False"
				  Margin="5">
				<Line StartPoint="0,10"
					  EndPoint="0,630"
